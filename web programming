<html>
<head>
<title>Recursion</title>
</head>
<body>
<script>
var n, f;
n = parseInt(prompt("Enter a number", "0"));

function fact(n) {
  if (n == 0)
    return 1;
  else
    return n * fact(n - 1);
}

f = fact(n);
document.writeln("Factorial of given number = " + f);
</script>
</body>
</html>
-----------------------------------------------------------------
<html>
<head>
<title>Swap Two Numbers</title>
</head>
<body>
<script>
var a, b;
a = parseInt(prompt("Enter a value", "0"));
b = parseInt(prompt("Enter b value", "0"));

document.writeln("Before Swapping<br>");
document.writeln("a = " + a + "<br>");
document.writeln("b = " + b + "<br>");

a = a + b;
b = a - b;
a = a - b;

document.writeln("<br>After Swapping<br>");
document.writeln("a = " + a + "<br>");
document.writeln("b = " + b);
</script>
</body>
</html>
--------------------------------------------------
<html>
<head>
<title>Prime Number Check</title>
</head>
<body>
<script>
var n, i, count = 0;
n = parseInt(prompt("Enter a number", "0"));

for(i = 1; i <= n; i++) {
  if (n % i == 0)
    count++;
}

if (count == 2)
  document.writeln("Given number is prime");
else
  document.writeln("Given number is not prime");
</script>
</body>
</html>



